{
  "name": "mock-service-builder",
  "version": "0.0.1",
  "main": "builder.js",
  "type": "module",
  "scripts": {
      "test": "prettier -c *.ts && eslint *.ts && ava",
      "clean": "rm -rf node_modules *.js",
      "fix": "prettier -w *.ts",
      "test:watch": "ava -w",
      "prepare": "tsc",
      "prepack": "npm test"
  },
  "devDependencies": {
      "@ava/typescript": "^3.0.1",
      "@typescript-eslint/eslint-plugin": "^5.53.0",
      "ava": "^5.1.0",
      "eslint": "^8.31.0",
      "msw": "^0.49.2",
      "prettier": "^2.8.1",
      "typescript": "^4.9.4"
  },
  "ava": {
      "files": [
          "!*.ts"
      ]
  },
  "eslintConfig": {
      "env": {
          "browser": true,
          "es2021": true
      },
      "extends": [
          "eslint:recommended",
          "plugin:@typescript-eslint/recommended"
      ],
      "overrides": [],
      "parser": "@typescript-eslint/parser",
      "parserOptions": {
          "ecmaVersion": "latest",
          "sourceType": "module"
      },
      "plugins": [
          "@typescript-eslint"
      ],
      "rules": {}
  },
  "author": "Matthijs van Henten <mvhenten@amazon.com>",
  "license": "MIT"
}
